{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"composite": true,
		"noEmitOnError": true,
		"experimentalDecorators": true,
		"target": "es6",
		"moduleResolution": "node",
		"module": "commonjs",
		"outDir": "dist",
		"strict": true,
		"jsx": "react",
		"incremental": true,
		"lib": [
			"esnext",
			"dom"
		]
	}
}
