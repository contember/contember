{
  "name": "@contember/client-content-generator",
  "license": "Apache-2.0",
  "version": "2.1.0-alpha.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/production/index.js",
  "bin": {
    "contember-client-generator": "./dist/production/generate.cjs"
  },
  "exports": {
    ".": {
