{
	"name": "@contember/database-migrations",
	"version": "1.4.0-rc.4",
	"scripts": {
		"test": "vitest --dir ./tests/cases",
		"build": "yarn build:js:dev && yarn build:js:prod",
		"build:js:dev": "NODE_ENV=development vite build --mode development",
		"build:js:prod": "vite build --mode production"
	},
	"license": "Apache-2.0",
	"main": "./dist/production/index.js",
	"typings": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/types/index.d.ts",
				"development": "./dist/development/index.js",
				"production": "./dist/production/index.js",
				"typescript": "./src/index.ts",
				"default": "./dist/production/index.js"
			},
			"require": {
				"types": "./dist/types/index.d.ts",
				"development": "./dist/development/index.cjs",
				"production": "./dist/production/index.cjs",
				"typescript": "./src/index.ts",
				"default": "./dist/production/index.cjs"
			}
		}
	},
	"dependencies": {
		"@contember/database": "workspace:*",
		"node-pg-migrate": "^5.10.0"
	},
	"devDependencies": {
		"pg": "^8.11.3"
	},
	"peerDependencies": {
		"pg": "^8.9.0"
	},
	"type": "module",
	"files": [
		"dist/",
		"src/"
	]
}
