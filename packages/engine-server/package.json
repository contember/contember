{
	"name": "@contember/engine-server",
	"version": "1.4.0-rc.4",
	"license": "Apache-2.0",
	"main": "./dist/production/index.js",
	"typings": "./dist/types/index.d.ts",
	"scripts": {
		"test-e2e": "vitest --dir ./tests/e2e --no-threads",
		"build": "yarn build:js:dev && yarn build:js:prod",
		"build:js:dev": "NODE_ENV=development vite build --mode development",
		"build:js:prod": "vite build --mode production"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/types/index.d.ts",
				"development": "./dist/development/index.js",
				"production": "./dist/production/index.js",
				"typescript": "./src/index.ts",
				"default": "./dist/production/index.js"
			},
			"require": {
				"types": "./dist/types/index.d.ts",
				"development": "./dist/development/index.cjs",
				"production": "./dist/production/index.cjs",
				"typescript": "./src/index.ts",
				"default": "./dist/production/index.cjs"
			}
		}
	},
	"dependencies": {
		"@contember/engine-actions": "workspace:*",
		"@contember/engine-http": "workspace:*",
		"@contember/engine-plugins": "workspace:*",
		"@contember/engine-s3-plugin": "workspace:*",
		"@contember/engine-vimeo-plugin": "workspace:*",
		"graphql": "^16.8.1",
		"pg": "^8.11.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@contember/engine-api-tester": "workspace:*",
		"@types/koa-compress": "^4.0.6",
		"@types/node": "^20.9.0",
		"@types/supertest": "^2.0.16",
		"supertest": "^6.3.3"
	},
	"type": "module",
	"files": [
		"dist/",
		"src/"
	]
}
