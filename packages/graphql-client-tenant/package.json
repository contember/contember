        "typescript": "./src/index.ts",
        "default": "./dist/production/index.cjs"
      }
    }
  },
  "dependencies": {
    "graphql-ts-client-api": "^3.1.17"
  },
  "devDependencies": {
    "@contember/engine-tenant-api": "workspace:*",
    "@graphql-tools/schema": "^10.0.7",
    "@types/node": "^20.17.22",
    "graphql": "^16.9.0",
    "graphql-ts-client-codegen": "^3.1.17"
